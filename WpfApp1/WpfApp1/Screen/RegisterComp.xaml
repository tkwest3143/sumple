<Page x:Class="WpfApp1.Screen.RegisterComp"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp1.Screen"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="RegisterComp">

    <Grid>
        <Grid HorizontalAlignment="Left" Height="323" VerticalAlignment="Top" Width="538" Background="#FFFDFBFB">
            <Button x:Name="LoginMoveButton" Content="ログイン画面へ移動" HorizontalAlignment="Center" VerticalAlignment="Center" Width="170" Height="43" Margin="0,200,0,0" Click="LoginMoveButton_Click"/>
            <Label Content="登録が完了しました。" HorizontalAlignment="Center" VerticalAlignment="Center" Height="49" Width="220" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="18" BorderThickness="0,0,0,1" BorderBrush="Black"/>
            <StatusBar HorizontalAlignment="Left" Height="33" Margin="0,290,0,0" VerticalAlignment="Top" Width="538"/>
        </Grid>

    </Grid>
</Page>
